{
  "name": "smartbox/camel-config-bundle",
  "description": "Smartbox Camel Config Bundle",
  "type": "symfony-bundle",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Smartbox\\Integration\\CamelConfigBundle\\": ""
    }
  },
  "require": {
    "php": ">=5.6",
    "ext-pcntl": "*",
    "smartbox/integration-framework-bundle": "^1.37.0",
    "symfony/framework-bundle": "^2.8|^3.0|^4.0",
    "symfony/yaml": "^2.8|^3.0|^4.0"
  },
  "require-dev": {
    "doctrine/doctrine-bundle": "^1.6",
    "doctrine/orm": "^2.5",
    "jms/serializer-bundle": "*",
    "symfony/monolog-bundle": "^2.4.0",
    "symfony/phpunit-bridge": "*",
    "stomp-php/stomp-php": "^4.4"
  },
  "config": {
    "bin-dir": "bin"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
